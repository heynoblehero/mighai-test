  "html_code": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>CloudDeploy - Simplified Cloud Deployment Platform</title>\n    <style>\n        :root {\n            --primary: #2563eb;\n            --secondary: #1e40af;\n            --text: #1f2937;\n            --light: #f3f4f6;\n        }\n        * { margin: 0; padding: 0; box-sizing: border-box; }\n        body {\n            font-family: -apple-system, system-ui, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, sans-serif;\n            line-height: 1.6;\n            color: var(--text);\n        }\n        .container {\n            max-width: 1200px;\n            margin: 0 auto;\n            padding: 0 1rem;\n        }\n        #heroSection {\n            background: linear-gradient(135deg, #1e40af 0%, #3b82f6 100%);\n            color: white;\n            padding: 4rem 0;\n            text-align: center;\n        }\n        .hero-content h1 {\n            font-size: 3rem;\n            margin-bottom: 1rem;\n        }\n        .hero-content p {\n            font-size: 1.25rem;\n            margin-bottom: 2rem;\n        }\n        #deploymentInstructions {\n            background: var(--light);\n            padding: 3rem 0;\n        }\n        .code-block {\n            background: #1f2937;\n            color: white;\n            padding: 1rem;\n            border-radius: 0.5rem;\n            margin: 1rem 0;\n            position: relative;\n        }\n        .copy-btn {\n            position: absolute;\n            right: 1rem;\n            top: 1rem;\n            background: var(--primary);\n            color: white;\n            border: none;\n            padding: 0.5rem 1rem;\n            border-radius: 0.25rem;\n            cursor: pointer;\n        }\n        #featuresSection {\n            padding: 4rem 0;\n        }\n        .features-grid {\n            display: grid;\n            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n            gap: 2rem;\n            margin-top: 2rem;\n        }\n        .feature-card {\n            padding: 1.5rem;\n            border-radius: 0.5rem;\n            box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.1);\n        }\n        #getStartedSection {\n            background: var(--light);\n            padding: 4rem 0;\n            text-align: center;\n        }\n        .cta-button {\n            display: inline-block;\n            background: var(--primary);\n            color: white;\n            padding: 1rem 2rem;\n            border-radius: 0.5rem;\n            text-decoration: none;\n            margin: 1rem;\n            transition: background 0.3s;\n        }\n        .cta-button:hover {\n            background: var(--secondary);\n        }\n    </style>\n</head>\n<body>\n    <section id=\"heroSection\">\n        <div class=\"container hero-content\">\n            <h1>Deploy Your Cloud Infrastructure in Minutes</h1>\n            <p>Streamlined deployment, powerful management, enterprise-grade security</p>\n        </div>\n    </section>\n\n    <section id=\"deploymentInstructions\">\n        <div class=\"container\">\n            <h2>Quick Start Deployment</h2>\n            <div class=\"code-block\">\n                <code>curl -sSL https://deploy.example.com/install | bash</code>\n                <button class=\"copy-btn\" onclick=\"copyToClipboard('curl -sSL https://deploy.example.com/install | bash')\">Copy</button>\n            </div>\n        </div>\n    </section>\n\n    <section id=\"featuresSection\">\n        <div class=\"container\">\n            <h2>Platform Features</h2>\n            <div class=\"features-grid\">\n                <div class=\"feature-card\">\n                    <h3>One-Click Deployment</h3>\n                    <p>Deploy your infrastructure with a single command</p>\n                </div>\n                <div class=\"feature-card\">\n                    <h3>Advanced Security</h3>\n                    <p>Enterprise-grade security and compliance built-in</p>\n                </div>\n                <div class=\"feature-card\">\n                    <h3>Real-time Monitoring</h3>\n                    <p>Monitor your infrastructure in real-time</p>\n                </div>\n            </div>\n        </div>\n    </section>\n\n    <section id=\"getStartedSection\">\n        <div class=\"container\">\n            <h2>Ready to Get Started?</h2>\n            <a href=\"/admin\" id=\"adminLink\" class=\"cta-button\">Access Admin Panel</a>\n            <a href=\"/subscribe/signup\" id=\"customerLink\" class=\"cta-button\">Create Account</a>\n        </div>\n    </section>\n\n    <script>\n        function copyToClipboard(text) {\n            navigator.clipboard.writeText(text).then(() => {\n                showSuccess('Command copied to clipboard!');\n            }).catch(err => {\n                showError('Failed to copy command');\n                console.error('Copy failed:', err);\n            });\n        }\n\n        async function checkSystemStatus() {\n            try {\n                setLoading(true);\n                const response = await fetch('/api/health');\n                const data = await response.json();\n                if (data.status === 'healthy') {\n                    showSuccess('System is running normally');\n                } else {\n                    showError('System status check failed');\n                }\n            } catch (error) {\n                showError('Unable to check system status');\n                console.error('Status check failed:', error);\n            } finally {\n                setLoading(false);\n            }\n        }\n\n        // Initial system status check\n        document.addEventListener('DOMContentLoaded', () => {\n            checkSystemStatus();\n        });\n    </script>\n</body>\n</html>",
