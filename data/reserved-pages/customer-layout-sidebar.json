  "html_code": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Customer Dashboard</title>\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css\">\n    <style>\n        .sidebar {\n            width: 280px;\n            height: 100vh;\n            background: white;\n            color: #64748b;\n            position: fixed;\n            left: 0;\n            top: 0;\n            padding: 1.5rem;\n            box-shadow: 0 0 20px rgba(0,0,0,0.05);\n        }\n\n        #sidebarHeader {\n            padding: 0.5rem 0 1.5rem;\n            border-bottom: 1px solid #e2e8f0;\n        }\n\n        #sidebarNav {\n            margin-top: 1.5rem;\n        }\n\n        .nav-item {\n            padding: 0.875rem 1rem;\n            margin: 0.25rem 0;\n            border-radius: 0.75rem;\n            cursor: pointer;\n            transition: all 0.2s ease;\n            color: #64748b;\n            text-decoration: none;\n            display: flex;\n            align-items: center;\n            gap: 12px;\n            font-weight: 500;\n        }\n\n        .nav-item:hover {\n            background-color: #f8fafc;\n            color: #0f172a;\n        }\n\n        .nav-item.active {\n            background-color: #f1f5f9;\n            color: #2563eb;\n        }\n\n        #logoutBtn {\n            position: absolute;\n            bottom: 2rem;\n            left: 1.5rem;\n            right: 1.5rem;\n            padding: 0.875rem;\n            background-color: #f1f5f9;\n            color: #64748b;\n            border: none;\n            border-radius: 0.75rem;\n            cursor: pointer;\n            transition: all 0.2s ease;\n            display: flex;\n            align-items: center;\n            justify-content: center;\n            gap: 8px;\n            font-weight: 500;\n        }\n\n        #logoutBtn:hover {\n            background-color: #e2e8f0;\n            color: #0f172a;\n        }\n\n        .user-welcome {\n            font-size: 0.875rem;\n            color: #64748b;\n            margin-bottom: 0.5rem;\n            display: flex;\n            align-items: center;\n            gap: 8px;\n        }\n\n        .brand-logo {\n            display: flex;\n            align-items: center;\n            gap: 12px;\n            margin-bottom: 1.5rem;\n            color: #0f172a;\n        }\n\n        .brand-logo i {\n            color: #2563eb;\n            font-size: 1.5rem;\n        }\n\n        .brand-logo h2 {\n            font-size: 1.25rem;\n            font-weight: 600;\n        }\n\n        .nav-icon {\n            width: 20px;\n            text-align: center;\n            color: #94a3b8;\n        }\n\n        .active .nav-icon {\n            color: #2563eb;\n        }\n\n        .nav-label {\n            font-size: 0.75rem;\n            text-transform: uppercase;\n            color: #94a3b8;\n            margin: 1.5rem 0 0.5rem 0.5rem;\n            letter-spacing: 0.05em;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"sidebar\">\n        <div id=\"sidebarHeader\">\n            <div class=\"brand-logo\">\n                <i class=\"fas fa-cube\"></i>\n                <h2>Customer Portal</h2>\n            </div>\n            <div class=\"user-welcome\" id=\"userWelcome\">\n                <i class=\"fas fa-user-circle\"></i>\n                Welcome!\n            </div>\n        </div>\n\n        <nav id=\"sidebarNav\">\n            <div class=\"nav-label\">Main Menu</div>\n            <a href=\"/dashboard\" class=\"nav-item\">\n                <i class=\"nav-icon fas fa-home\"></i>\n                Dashboard\n            </a>\n            <a href=\"/dashboard/profile\" class=\"nav-item\">\n                <i class=\"nav-icon fas fa-user\"></i>\n                Profile\n            </a>\n            <div class=\"nav-label\">Subscription</div>\n            <a href=\"/dashboard/upgrade\" class=\"nav-item\">\n                <i class=\"nav-icon fas fa-credit-card\"></i>\n                Billing\n            </a>\n            <a href=\"/dashboard/upgrade\" class=\"nav-item\">\n                <i class=\"nav-icon fas fa-arrow-up\"></i>\n                Upgrade Plan\n            </a>\n        </nav>\n\n        <button id=\"logoutBtn\" onclick=\"handleSidebarLogout()\">\n            <i class=\"fas fa-sign-out-alt\"></i>\n            Logout\n        </button>\n    </div>\n\n    <script>\n        const route_mapping = {\n            profile: \"/dashboard/profile\",\n            settings: \"/dashboard/profile\",\n            billing: \"/dashboard/upgrade\",\n            upgrade: \"/dashboard/upgrade\",\n            dashboard: \"/dashboard\",\n            home: \"/\",\n            login: \"/subscribe/login\",\n            signup: \"/subscribe/signup\"\n        };\n\n        async function handleSidebarLogout() {\n            try {\n                setLoading('logoutBtn', true);\n                const response = await fetch('/api/auth/logout', {\n                    method: 'POST',\n                    credentials: 'include'\n                });\n                \n                if (response.ok) {\n                    window.location.href = route_mapping.login;\n                } else {\n                    showError('Logout failed. Please try again.');\n                }\n            } catch (error) {\n                showError('Network error during logout');\n            } finally {\n                setLoading('logoutBtn', false);\n            }\n        }\n\n        async function loadUserInfo() {\n            try {\n                const response = await fetch('/api/subscribe/me', {\n                    credentials: 'include'\n                });\n                \n                if (response.ok) {\n                    const userData = await response.json();\n                    document.getElementById('userWelcome').innerHTML = `\n                        <i class=\"fas fa-user-circle\"></i>\n                        Welcome, ${sanitizeInput(userData.username)}!\n                    `;\n                }\n            } catch (error) {\n                console.error('Error loading user info');\n            }\n        }\n\n        function updateActiveNav() {\n            const currentPath = window.location.pathname;\n            const navItems = document.querySelectorAll('.nav-item');\n            \n            navItems.forEach(item => {\n                item.classList.remove('active');\n                if (item.getAttribute('href') === currentPath) {\n                    item.classList.add('active');\n                }\n            });\n        }\n\n        document.addEventListener('DOMContentLoaded', () => {\n            loadUserInfo();\n            updateActiveNav();\n        });\n\n        window.addEventListener('popstate', updateActiveNav);\n    </script>\n</body>\n</html>",
